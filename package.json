{
  "name": "heroku-nodejs-build-demo",
  "version": "0.0.1",
  "private": true,
  "repository": "https://github.com/darrinholst/heroku-nodejs-build-demo",
  "engines": {
    "node": "^8.8.1"
  },
  "scripty": {
    "silent": true
  },
  "prettier": {
    "printWidth": 80,
    "singleQuote": true,
    "bracketSpacing": false
  },
  "scripts": {
    "start":
      "NODE_CONFIG_DIR=\"./server/config\" node -r source-map-support/register server/dist/index.js",
    "lint": "SCRIPTY_PARALLEL=true scripty",
    "clean": "scripty",
    "dev": "scripty",
    "dev:browsersync": "scripty",
    "dev:serve": "scripty",
    "build:dev": "scripty",
    "build:prod": "scripty",
    "build:client:dev": "scripty",
    "build:client:prod": "scripty",
    "build:client:watch": "scripty",
    "build:server": "scripty",
    "build:server:watch": "scripty",
    "db:migrate": "db-migrate up --env ${NODE_ENV:?} --migrations-dir server/config/database/migrations --config server/config/database/database.json && db-migrate up:${NODE_ENV} --env ${NODE_ENV:?} --migrations-dir server/config/database/migrations --config server/config/database/database.json",
    "db:start": "scripty",
    "db:dev:migrate": "scripty",
    "db:dev:reset": "scripty",
    "db:dev:start": "scripty",
    "db:prod:migrate": "scripty",
    "db:staging:migrate": "scripty",
    "db:test:reset": "scripty",
    "logs:staging": "heroku logs -t -n 1000 -s app -d web -a hnbd-staging",
    "logs:prod": "heroku logs -t -n 1000 -s app -d web -a hnbd-production",
    "test": "scripty",
    "test:integration": "scripty",
    "test:integration:server": "scripty",
    "test:unit": "scripty",
    "test:unit:client": "scripty",
    "test:unit:client:watch": "scripty",
    "test:unit:server": "scripty",
    "test:unit:server:watch": "scripty",
    "test:unit:universal": "scripty",
    "test:e2e": "scripty",
    "verify": "scripty",
    "package": "node_modules/heroku-nodejs-build/scripts/package",
    "release": "node_modules/heroku-nodejs-build/scripts/release",
    "deploy": "node_modules/heroku-nodejs-build/scripts/deploy"
  },
  "dependencies": {
    "async": "^2.5.0",
    "body-parser": "^1.17.2",
    "compression": "^1.6.2",
    "config": "^1.27.0",
    "db-migrate": "^0.10.0-beta.20",
    "db-migrate-pg": "^0.2.4",
    "debug": "^3.0.0",
    "express": "^4.15.3",
    "express-jwt": "^5.3.0",
    "glob": "^7.1.2",
    "hbs": "^4.0.1",
    "jsonwebtoken": "^8.0.1",
    "morgan": "^1.8.2",
    "pg-native": "^2.0.1",
    "pg-promise": "^7.0.0",
    "source-map-support": "^0.5.0",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "@angular/animations": "^4.3.0",
    "@angular/cdk": "^2.0.0-beta.11",
    "@angular/common": "^4.3.0",
    "@angular/compiler": "^4.3.0",
    "@angular/core": "^4.3.0",
    "@angular/flex-layout": "2.0.0-beta.9",
    "@angular/forms": "^4.3.0",
    "@angular/http": "^4.3.0",
    "@angular/material": "^2.0.0-beta.11",
    "@angular/platform-browser": "^4.3.0",
    "@angular/platform-browser-dynamic": "^4.3.0",
    "@angular/router": "^4.3.0",
    "@ngrx/effects": "^4.0.5",
    "@ngrx/router-store": "^4.0.4",
    "@ngrx/store": "^4.0.3",
    "@ngrx/store-devtools": "^4.0.0",
    "@types/chai": "^4.0.1",
    "@types/debug": "^0.0.30",
    "@types/lodash": "^4.14.70",
    "@types/mocha": "^2.2.41",
    "@types/node": "^8.0.26",
    "@types/uuid": "^3.4.2",
    "angular2-jwt": "^0.2.3",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.2.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^8.0.0",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.6.0",
    "browser-sync": "^2.18.12",
    "chai": "^4.1.0",
    "chai-as-promised": "^7.1.1",
    "copy-webpack-plugin": "^4.2.0",
    "css-loader": "^0.28.4",
    "cucumber": "^3.0.1",
    "dirty-chai": "^2.0.0",
    "eslint": "^4.2.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-prettier": "^2.2.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.4",
    "font-awesome": "^4.7.0",
    "foreman": "^2.0.0",
    "hammerjs": "^2.0.8",
    "html-loader": "^0.5.0",
    "jsdom": "^9.12.0",
    "lodash": "^4.17.4",
    "mocha": "4.0.1",
    "mocha-webpack": "^0.7.0",
    "ngrx-store-freeze": "^0.2.0",
    "ngrx-store-logger": "^0.2.0",
    "ngx-validators": "^3.0.0",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "prettier": "^1.7.0",
    "protractor": "^5.1.2",
    "protractor-cucumber-framework": "^4.0.7",
    "raw-loader": "^0.5.1",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.4.2",
    "sass-loader": "^6.0.5",
    "scripty": "^1.7.1",
    "style-loader": "0.19.0",
    "stylelint": "^8.1.0",
    "stylelint-config-standard": "~17.0.0",
    "tslint": "^5.5.0",
    "typescript": "^2.4.1",
    "url-loader": "0.6.2",
    "webpack": "^3.5.4",
    "webpack-config-utils": "^2.3.0",
    "webpack-node-externals": "^1.6.0",
    "zone.js": "^0.8.13"
  }
}
